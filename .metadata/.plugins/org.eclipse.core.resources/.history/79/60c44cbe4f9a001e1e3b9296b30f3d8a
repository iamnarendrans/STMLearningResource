#include "FIR.h"

void FIRFilter_Init(FIRFilter *filt, float *coeff, float *buf, const uint8_t order)
{
	filt->out = 0.0f;
	filt->coeff = coeff;
	filt->buf = buf;
	filt->order = order;
}

void FIRFilter_update(FIRFilter *filt, float In);
