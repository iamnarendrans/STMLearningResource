#ifndef PID_H_
#define PID_H_


typedef struct
{
	//Controller gains
	float Kp;
	float Ki;
	float Kd;


	// Derivative low-pass filter timer constant
	float tau;


	// Output limits
	float limMin;
	float limMax;


}PIDController;


#endif /* PID_H_ */
